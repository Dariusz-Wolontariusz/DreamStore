{
  "name": "dreamshop",
  "version": "1.0.0",
  "description": "My eCommerce project built with MERN",
  "main": "server.js",
  "engines": {
    "node" : "14.x"
  },
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dariusz-Wolontariusz/DreamStore-frontend.git"
  },
  "author": "Dariusz Ciazynski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dariusz-Wolontariusz/DreamStore-frontend/issues"
  },
  "homepage": "https://github.com/Dariusz-Wolontariusz/DreamStore-frontend#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
